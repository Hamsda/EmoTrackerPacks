{
  "tracker_right_map": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "v_alignment": "stretch",
          "margin": "0,0,3,0",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#FF212121",
                "layout": "options_popup"
              },
              "content": {
                "type": "layout",
                "key": "item_grid_tall"
              }
            },
            {
              "type": "group",
              "header": "Counters",
              "dock": "top",
              "content": {
                "type": "layout",
                "key": "counters_tall"
              }
            }
          ]
        },
        {
          "type": "dock",
          "content": [
            {
              "type": "tabbed",
              "dock": "top",
              "tabs": [
                {
                  "title": "One Map",
                  "icon": "images/map.png",
                  "content": {
                    "type": "tabbed",
                    "tabs": [
                      {
                        "title": "Locations",
                        "content": {
                          "type": "map",
                          "maps": [
                            "locations"
                          ]
                        }
                      },
                      {
                        "title": "Exits",
                        "content": {
                          "type": "map",
                          "maps": [
                            "exits"
                          ]
                        }
                      }
                    ]
                  }
                },
                {
                  "title": "Two Maps",
                  "icon": "images/map.png",
                  "content": {
                    "type": "array",
                    "orientation": "vertical",
                    "content": [
                      {
                        "type": "tabbed",
                        "tabs": [
                          {
                            "title": "Locations",
                            "content": {
                              "type": "map",
                              "maps": [
                                "locations"
                              ]
                            }
                          },
                          {
                            "title": "Exits",
                            "content": {
                              "type": "map",
                              "maps": [
                                "exits"
                              ]
                            }
                          }
                        ]
                      },
                      {
                        "type": "tabbed",
                        "tabs": [
                          {
                            "title": "Locations",
                            "content": {
                              "type": "map",
                              "maps": [
                                "locations"
                              ]
                            }
                          },
                          {
                            "title": "Exits",
                            "content": {
                              "type": "map",
                              "maps": [
                                "exits"
                              ]
                            }
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "horizontal",
                "compact": true
              }
            }
          ]
        }
      ]
    }
  },
  "tracker_top_map": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "bottom",
          "content": [
            {
              "type": "dock",
              "dock": "left",
              "margin": "0,0,3,0",
              "content": [
                {
                  "type": "group",
                  "header": "Items",
                  "dock": "top",
                  "header_content": {
                    "type": "button_popup",
                    "style": "settings",
                    "popup_background": "#FF212121",
                    "layout": "options_popup"
                  },
                  "content": {
                    "type": "layout",
                    "key": "item_grid_wide"
                  }
                },
                {
                  "type": "group",
                  "header": "Counters",
                  "dock": "top",
                  "content": {
                    "type": "layout",
                    "key": "counters_wide"
                  }
                }
              ]
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "horizontal",
                "compact": true
              }
            }
          ]
        },
        {
          "type": "tabbed",
          "tabs": [
            {
              "title": "Locations",
              "content": {
                "type": "map",
                "maps": [
                  "locations"
                ]
              }
            },
            {
              "title": "Exits",
              "content": {
                "type": "map",
                "maps": [
                  "exits"
                ]
              }
            }
          ]
        }
      ]
    }
  }
}