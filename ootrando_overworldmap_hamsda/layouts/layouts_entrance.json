{
  "tracker_very_tall": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "v_alignment": "stretch",
          "margin": "0,0,3,0",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#FF212121",
                "layout": "options_popup"
              },
              "content": {
                "type": "layout",
                "key": "item_grid_tall"
              }
            },
            {
              "type": "group",
              "header": "Counters",
              "dock": "top",
              "content": {
                "type": "layout",
                "key": "counters_tall"
              }
            },
            {
              "type": "group",
              "header": "Presets",
              "dock": "top",
              "content": {
                "type": "item",
                "item": "presets",
                "h_alignment": "left",
                "v_alignment": "top",
                "margin": "5",
                "width": 32,
                "height": 32
              }
            }
          ]
        },
        {
          "type": "dock",
          "content": [
            {
              "type": "tabbed",
              "dock": "top",
              "tabs": [
                {
                  "title": "Locations",
                  "icon": "images/chest_brown_closed.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "locations"
                    ]
                  }
                },
                {
                  "title": "Exits",
                  "icon": "images/door.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "exits"
                    ]
                  }
                },
                {
                  "title": "Two Maps",
                  "icon": "images/map.png",
                  "content": {
                    "type": "array",
                    "orientation": "vertical",
                    "content": [
                      {
                        "type": "map",
                        "orientation": "vertical",
                        "maps": [
                          "locations",
                          "exits"
                        ]
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "horizontal",
                "compact": true
              }
            }
          ]
        }
      ]
    }
  },
  "tracker_wide_with_scrollable_pins": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "margin": "0,0,3,0",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#FF212121",
                "layout": "options_popup"
              },
              "content": {
                "type": "layout",
                "key": "item_grid_wide"
              }
            },
            {
              "type": "group",
              "header": "Counters",
              "dock": "top",
              "content": {
                "type": "layout",
                "key": "counters_wide"
              }
            },
            {
              "type": "group",
              "header": "Presets",
              "dock": "top",
              "content": {
                "type": "item",
                "item": "presets",
                "h_alignment": "left",
                "v_alignment": "top",
                "margin": "5",
                "width": 32,
                "height": 32
              }
            }
          ]
        },
        {
          "type": "dock",
          "content": [
            {
              "type": "group",
              "header": "Pinned Locations",
              "dock": "left",
              "width": 200,
              "content": [
                {
                  "type": "scroll",
                  "horizontal_scrollbar_visibility": "disabled",
                  "content": {
                    "type": "recentpins",
                    "style": "wrap",
                    "h_alignment": "stretch",
                    "v_alignment": "stretch",
                    "orientation": "vertical",
                    "compact": true
                  }
                }
              ]
            },
            {
              "type": "tabbed",
              "tabs": [
                {
                  "title": "Locations",
                  "icon": "images/chest_brown_closed.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "locations"
                    ]
                  }
                },
                {
                  "title": "Exits",
                  "icon": "images/door.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "exits"
                    ]
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  }
}